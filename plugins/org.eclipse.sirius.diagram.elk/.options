# Tracing options for the org.eclipse.sirius.diagram.elk plug-in

# Activate the export of elk diagram in the directory set by JVM option -Djava.io.tmpdir=PATH
org.eclipse.sirius.diagram.elk/debug=false
org.eclipse.sirius.diagram.elk/debug/as_text=false
# If not set => temp dir
# if absolute => workspace
# otherwise => relative to AIRD file
org.eclipse.sirius.diagram.elk/debug/path=
