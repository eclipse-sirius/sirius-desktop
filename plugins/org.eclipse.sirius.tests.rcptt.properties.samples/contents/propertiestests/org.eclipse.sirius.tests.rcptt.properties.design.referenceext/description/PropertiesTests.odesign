      <controls xsi:type="properties-ext-widgets-reference:ExtReferenceDescription" labelExpression="Mono ref" referenceNameExpression="reference" semanticCandidateExpression="var:self"/>
      <controls xsi:type="properties-ext-widgets-reference:ExtReferenceDescription" labelExpression="Multi ref" referenceNameExpression="references" semanticCandidateExpression="var:self"/>
